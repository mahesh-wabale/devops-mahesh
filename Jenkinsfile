pipeline {
  agent any
  stages {
    stage('Checkout') {
      steps {
        echo 'pwd'
      }
    }

    stage('Build') {
      steps {
        echo 'pwd'
      }
    }

    stage('Test') {
      steps {
        echo 'pwd'
      }
    }

    stage('Deploy in Production') {
      steps {
        echo 'pwd'
        echo 'Initiating deployment'
        
      }
    }

  }
}
